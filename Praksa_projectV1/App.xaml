<Application x:Class="Praksa_projectV1.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Praksa_projectV1" xmlns:sys="clr-namespace:System;assembly=netstandard"
             Startup="ApplicationStart">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Styles/UIColors.xaml" />
                <ResourceDictionary Source="/Styles/ButtonStyles.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <sys:String x:Key="StartDate">{x:Static sys:DateTime.Today.AddMonths(-1).ToString("dd-MM-yyyy")}</sys:String>
            <sys:String x:Key="EndDate">{x:Static sys:DateTime.Today.ToString("dd-MM-yyyy")}</sys:String>
        </ResourceDictionary>
        

    </Application.Resources>
</Application>
